<?xml version="1.0" encoding="UTF-8"?>
<AssignmentRules xmlns="http://soap.sforce.com/2006/04/metadata">
    <assignmentRule>
        <fullName>Lead Owner Assignment</fullName>
        <active>true</active>
        <ruleEntry>
            <assignedTo>WebLeads_Queue</assignedTo>
            <assignedToType>Queue</assignedToType>
            <criteriaItems>
                <field>Lead.LeadSource</field>
                <operation>equals</operation>
                <value>Website</value>
            </criteriaItems>
            <criteriaItems>
                <field>Lead.RecordTypeId</field>
                <operation>equals</operation>
                <value>WebLeads</value>
            </criteriaItems>
        </ruleEntry>
    </assignmentRule>
</AssignmentRules>
